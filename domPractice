// app.appendChild(table)

const orderedList = document.createElement('ol');

// Sample data for list items
const items = ['First item', 'Second item', 'Third item'];

// Create <li> elements and append them to the <ol>
items.forEach(itemText => {
    const listItem = document.createElement('li');
    listItem.textContent = itemText; // Set the text content of the <li>
    orderedList.appendChild(listItem); // Append the <li> to the <ol>
});

// Append the <ol> to the document body (or any other desired element)
document.body.appendChild(orderedList);